# Pollen ID - Environment Variables Template
# ===========================================
# Copy this file to '.env' and fill in your actual API keys
# NEVER commit the .env file to version control

# Google Pollen API Configuration
# Get your key at: https://console.cloud.google.com
GOOGLE_API_KEY=your_google_pollen_api_key_here

# OpenAI API Configuration
# Get your key at: https://platform.openai.com
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Default Location
DEFAULT_LOCATION=10001

# Optional: App Configuration
APP_TITLE=Pollen ID
APP_VERSION=1.0.0
DEBUG_MODE=False
